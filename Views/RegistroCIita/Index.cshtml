@model List<ProyectoCFP.Models.RegistrarCita>

<h2>Lista de Citas</h2>

<p>
    <a asp-action="Crear" class="btn btn-success">Crear Nueva Cita</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Apellido Paterno</th>
            <!-- Agrega aquí las otras columnas que quieras mostrar -->
        </tr>
    </thead>
    <tbody>
        @foreach (var cita in Model)
        {
            <tr>
                <td>@cita.Id</td>
                <td>@cita.Nombre</td>
                <td>@cita.ApellidoPaterno</td>
                <!-- Agrega aquí las otras columnas que quieras mostrar -->
            </tr>
        }
    </tbody>
</table>



